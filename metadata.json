{
  "name": "peelman-ubnt",
  "version": "1.0.0",
  "author": "Nick Peelman <nick@peelman.us>",
  "summary": "Provides an interface for managing Ubiquiti UniFi Apt source, key, and the UniFi Package",
  "license": "Apache-2.0",
  "source": "https://github.com/peelman/peelman-unifi",
  "project_page": "https://github.com/peelman/peelman-unifi",
  "issues_url": "https://github.com/peelman/peelman-unifi/issues",
  "operatingsystem_support": [
    {
